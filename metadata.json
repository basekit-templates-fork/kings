        },
        "button": {
            "color__button--background" : "@color__secondary",
            "color__button--text" : "#7e8a8e",
            "color__button--background--hover": "darken(@color__button--background, 4%)",
            "color__button--text--hover" : "@color__button--text",
            "color__button--border" : "@font-swatch7-color",
            "font__button--font-family" : "@font-swatch7-font-family",
            "font__button--font-size" : "13px",
            "font__button--font-weight" : "700",
            "font__button--line-height" : "44px",
            "font__button--letter-spacing" : "2px"
        },
        "gallery": {
            "color__gallery-caption--background" : "fade(@color__p--text, 50%)",
            "color__gallery-title--text" : "@color__body--background",
            "color__gallery-description--text" : "@color__body--background"
        },
        "slideshow": {
            "color__slideshow-caption--background" : "fade(@color__body--background, 80%)",
            "color__slideshow-title--text" : "@color__p--text",
            "color__slideshow-description--text" : "@color__p--text",
            "color__slideshow-pagination-item--background": "@color__secondary"
