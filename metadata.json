        "ecommerce": {
            "color__ecomproductslist-filters--background" : "@color__primary",
            "color__ecomproductslist-product-title--text" : "@color__p--text",
            "color__ecomproduct--background" : "@color__primary",
            "color__ecomproduct-images--background" : "lighten(desaturate(@color__primary, 80%), 14%)",
            "color__ecomproduct-title--text" : "@color__p--text",
            "color__ecomproduct-price--text" : "@color__p--text"
        }
